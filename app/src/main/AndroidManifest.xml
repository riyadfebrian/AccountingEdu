<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.education.accounting.accountingeducation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Instruction" />
        <activity android:name=".nama" />
        <activity android:name=".material.youtube" />
        <activity android:name=".material.hasilpengamatan" />
        <activity android:name=".material.pertanyaan" />
        <activity android:name=".material.temukanjawaban" />
        <activity android:name=".closing" />
        <activity android:name=".aboutus" />
        <activity android:name=".ki" />
        <activity android:name=".kd" />
        <activity android:name=".ip" />
        <activity android:name=".Menu" />
        <activity android:name=".exercise.QuizActivity" />
        <activity android:name=".exercise.TrueFalseQuiz" />
        <activity android:name=".exercise.Result" />

        <activitys
            android:name=".openWeb"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_open_web"
            android:theme="@style/FullscreenTheme" />

        <service
            android:name=".MusicBackground"
            android:enabled="true"
            android:stopWithTask="true" />

        <activity
            android:name=".exercise.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".exercise.Result"></activity>
    </application>

</manifest>